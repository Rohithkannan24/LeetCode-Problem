function isValid(s: string): boolean {
    let stack:string[]=[];
    const brackets= {
            ')': '(',
            ']': '[',
            '}': '{'
        };
    for (const char of s) {
            if ('([{'.includes(char)) {
              stack.push(char);
            } 
            else if (char in brackets) {
               if (stack.length===0 || stack.pop() !== brackets[char]) {
            return false;
            }
            }
        }

        return stack.length===0;    
};
