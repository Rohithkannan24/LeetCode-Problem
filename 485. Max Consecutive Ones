function findMaxConsecutiveOnes(nums: number[]): number {
    if (nums.length === 0){
        return 0; 
    }
    
    let l = 0;
    let maxLen = 0;
    
    for (let r = 0; r < nums.length; r++) {
        if (nums[r] === 0) {
            l = r + 1; 
        }
         else {
            maxLen = Math.max(maxLen, r - l + 1); 
        }
    }
    
    return maxLen;

    
};
