function firstUniqChar(s: string): number {
    const sArray = [...s];
    
    for (let i = 0; i < sArray.length; i++) {
        let uniqueValue = true;
        
        for (let j = 0; j < sArray.length; j++) {
            if (i !== j && sArray[i] === sArray[j]) {
                uniqueValue = false;
                break;
            }
        }
        
        if (uniqueValue) {
            return i;
        }
    }
    
    return -1;
};
