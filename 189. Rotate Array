function rotate(nums: number[], k: number): void {
    
    k = k % nums.length;
    reverse(nums,0,nums.length-1);
    reverse(nums,0,k-1);
    reverse(nums,k,nums.length-1);
    
 
    function reverse(value: number[], l: number, r: number): void {
       while (l < r) {
           let temp= value[l];
           value[l]=value[r];
           value[r]=temp;
           l++;
           r--;
        }
    }
  
};
