function guessNumber(n: number): number {
    let l=0;
    let r=n;
    while(l<=r){
       let m=Math.floor((l+r)/2);
       if(guess(m)===0){
          return m;
        }
        else if(guess(m)===1){
            l=m+1;
        }
        else {
            r=m-1;
        }
         
    }
    return -1;


};
