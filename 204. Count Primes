function countPrimes(n: number): number {
    if(n<=2){
        return 0;
    }
    let count=0;
    for(let i=2; i<n;i++){
        if(isPrime(i)){
            count++;
        }
    }
    

    function isPrime(num: number): boolean {
    if (num <= 1) return false;
    if (num === 2) return true;
    if (num % 2 === 0) return false;
    for (let i = 3; i * i <= num; i += 2) {
        if (num % i === 0) return false;
    }
    return true;
}
return count;
    
};
