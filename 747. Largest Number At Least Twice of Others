function dominantIndex(nums: number[]): number {
    let max=nums[0];
    let indexvalue=0;
    for(let i =0; i<nums.length;i++){
        if(nums[i]>max){
            max= nums[i];
            indexvalue=i;
        }
    }
    for(let i=0;i<nums.length;i++){
        if(i!==indexvalue && (nums[i]*2)>max){
            return -1;
        }
    }
    return indexvalue;
    
};
