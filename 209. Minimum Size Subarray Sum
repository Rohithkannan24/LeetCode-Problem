function minSubArrayLen(target: number, nums: number[]): number {
    let l=0;
    let r=0;
    let min=[];
    let sum = 0;
    while(r<nums.length){
        sum+=nums[r];
        while(sum>=target){
            min.push(r-l+1);
            sum-=nums[l];
            l++;
        }
        r++;
    }
    if(min.length===0){
        return 0;
    }
    else{
        return Math.min(...min);
    }
    
};
