function groupAnagrams(strs: string[]): string[][] {
    const res= new Map <string,string[]>();
    for(let s of strs){
        let key = s.split('').sort().join('');
        if(!res.has(key)){
            res.set(key,[]);
        }
        res.get(key).push(s);
    }
    return Array.from(res.values()); 
    
};
